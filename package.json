{
  "name": "cerebral-website",
  "version": "0.1.0",
  "description": "A boilerplate for running a Webpack workflow in Node express",
  "main": "server.js",
  "engines": {
    "node": "4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianalfoni/cerebral-website.git"
  },
  "keywords": [
    "cerebral",
    "mvc",
    "react",
    "spa"
  ],
  "author": "Christian Alfoni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christianalfoni/cerebral-website/issues"
  },
  "homepage": "http://www.cerebraljs.com/",
  "scripts": {
    "start": "node server_build",
    "serverbuild": "NODE_ENV=server webpack --config webpack.server.config.js --watch",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "postinstall": "node heroku"
  },
  "dependencies": {
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cerebral": "^0.34.1",
    "cerebral-model-baobab": "^0.4.8",
    "cerebral-module-http": "^0.2.0",
    "cerebral-module-router": "^0.14.4",
    "cerebral-view-react": "^0.11.17",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "markdown-to-react-components": "^0.2.0",
    "raw-loader": "^0.5.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
