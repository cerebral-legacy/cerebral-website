{
  "name": "cerebral-website",
  "version": "0.1.0",
  "description": "A boilerplate for running a Webpack workflow in Node express",
  "main": "server.js",
  "engines": {
    "node": "4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianalfoni/cerebral-website.git"
  },
  "keywords": [
    "cerebral",
    "mvc",
    "react",
    "spa"
  ],
  "author": "Christian Alfoni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christianalfoni/cerebral-website/issues"
  },
  "homepage": "http://www.cerebraljs.com/",
  "scripts": {
    "test": "",
    "start": "node server_build",
    "serverbuild": "NODE_ENV=server webpack --config webpack.server.config.js --watch",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "postinstall": "node heroku",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "dependencies": {
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.23",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.0.5",
    "cerebral": "^0.28.2",
    "cerebral-model-baobab": "^0.4.5",
    "cerebral-react": "^0.9.0",
    "cerebral-router": "^0.8.0",
    "css-loader": "^0.19.0",
    "express": "^4.13.3",
    "json-loader": "^0.5.3",
    "markdown-to-react-components": "^0.1.3",
    "postcss-loader": "^0.6.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "source-map-support": "^0.3.3",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "babel-eslint": "^4.1.3",
    "babel-jscs": "^2.0.4",
    "jscs": "^2.1.1",
    "jsdom": "^6.5.1",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "react-addons-test-utils": "^0.14.3"
  }
}
